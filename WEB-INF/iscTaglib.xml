  <display-name>Isomorphic Software</display-name>
  <tlib-version>1.1</tlib-version>
  <short-name>isc</short-name>
  <uri>http://www.smartclient.com/taglib</uri>

  <!-- 
       Note: the 'runat' attribute present on all tags below is there for tag compatibility
       with ASP.NET.  The 'runat' attribute is not used by java, but specifying it here allows
       you to to simply rename your .aspx pages to .jsp, change the taglib declaration in your
       file and go.
  -->
  <tag>
    <name>jsString</name>
    <tag-class>com.isomorphic.taglib.JSStringTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>filename</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>runat</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>

